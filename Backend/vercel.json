{
    "version":2,
    "builds":[
        {"src":"*.js","use":"@vercel/node"}
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest":"/",
            "methos":["GET","POST","OPTIONS"],
            "headers": {
                "Access-Control-Allow-Origin":"*"
            }
        }
        
    ]
   
}